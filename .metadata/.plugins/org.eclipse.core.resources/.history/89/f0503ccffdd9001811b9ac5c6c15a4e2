package br.com.fiap.common;

import java.util.List;

public class QuestaoVO {
	
	public QuestaoVO() {}
	
	private Integer id;

	private Integer codigoAvaliacao;

	private String descricao;
	
	private List<Questao> questoes;

	public QuestaoVO(Integer id, Integer codigoAvaliacao, String descricao, List<Questao> questoes) {
		this.id =id;
		this.codigoAvaliacao = codigoAvaliacao;
		this.descricao = descricao;
		this.questoes = questoes;
	}

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public Integer getCodigoAvaliacao() {
		return codigoAvaliacao;
	}

	public void setCodigoAvaliacao(Integer codigoAvaliacao) {
		this.codigoAvaliacao = codigoAvaliacao;
	}

	public String getDescricao() {
		return descricao;
	}

	public void setDescricao(String descricao) {
		this.descricao = descricao;
	}
	
	public List<Questao> getQuestoes() {
		return questoes;
	}

	public void setQuestoes(List<Questao> questoes) {
		this.questoes = questoes;
	}

}
